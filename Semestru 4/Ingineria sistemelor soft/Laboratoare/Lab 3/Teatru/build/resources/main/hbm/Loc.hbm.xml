<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hbm">

    <class name="com.example.baschetgui.cs.models.Loc" table="Locs">
        <id name="id" column="id">
        </id>
        <property name="name"/>
        <property name="email"/>
        <property name="seat"/>
        <property name="reserved" type="boolean"/>
        <property name="payed" type="boolean"/>
    </class>

</hibernate-mapping>
